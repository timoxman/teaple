<section id="main">

<% if Teaple.all.any? %>
  <ul>
  <% Teaple.all.each do |teaple|%>
    <li>
    <%= teaple.name %>
    <%= link_to "Arrange meeting", new_teaple_message_path(teaple) %>
    </li>
  <%  end %>
  </ul>
<% else %>
  <h2>No teaples added yet</h2>
<% end %>

<%= link_to "Add your profile", new_teaple_path %>
</section>

