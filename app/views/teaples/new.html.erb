<%= form_for @teaple, html: {multipart: true} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br/><br/>
  <%= f.label :bio %>
  <%= f.text_field :bio %><br/><br/>
  <%= f.label :location %>
  <%= f.text_field :location %><br/><br/>
  <%= f.label :postcode %>
  <%= f.text_field :postcode %><br/><br/>
  <%= f.file_field :image %> <br>
  <%= f.submit "Save profile" %>
<!--   <%= f.file_field :image %> -->
<% end %>
